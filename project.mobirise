ass=\"mbr-flex mbr-jc-b mbr-bold\">\n\t\t\t\t\t\t    <p class=\"mbr-fonts-style mbr-pb-1 prog-title5\" mbr-theme-style=\"display-7\" data-app-selector=\".prog-title5\">Construction Drawings</p>\n\t\t\t\t\t        <span class=\"progress-value5 mbr-fonts-style mbr-bold prog-title5\" mbr-theme-style=\"display-4\" data-app-selector=\".prog-title5\">%</span>\n\t\t\t\t\t    </div>\n                        <div class=\"progress-wrap\">\n                            <div class=\"progress progress5\"></div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-5\" mbr-if=\"showTabsBlock\" mbr-class=\"{'mbr-col-lg-10': !showProgressBlock,\n                             'mbr-col-lg-12': !showProgressBlock && !showTitle,\n                             'mbr-col-lg-6': !showTitle}\">\n                <amp-selector role=\"tablist\" layout=\"container\" class=\"ampTabContainer mbr-flex flex-wrap mbr-jc-sa\">\n                         <!-- TAB -->\n                    <div role=\"tab\" class=\"tabButton\" selected option=\"a\">\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"amp-iconfont animation-normal mbri-timer\"></span>\n                        </div>\n                        <h3 class=\"mbr-fonts-style mbr-bold text-upper mbr-pt-2 custom-block2-title\" mbr-theme-style=\"display-4\" data-app-selector=\".custom-block2-title\">GST Returns</h3>\n                    </div>\n                    <div role=\"tabpanel\" class=\"tabContent\">\n                        <p class=\"mbr-fonts-style custom-block2-text\" mbr-theme-style=\"display-7\" data-app-selector=\".custom-block2-text\">We can deal directly with IRD and take the workload off</p>\n                    </div>\n                         <!-- TAB -->\n                    <div role=\"tab\" class=\"tabButton\" option=\"b\">\n                        <div class